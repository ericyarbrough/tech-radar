{
  "env": {
    "es2021": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module"
  },
  "rules": {
    "class-methods-use-this": 0,
    "curly": 0,
    "no-case-declarations": 0,
    "no-console": 0,
    "no-continue": 0
  }
}
